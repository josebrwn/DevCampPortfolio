<div class="inner cover">
<h1 class="cover-heading">Tech News</h1>
<h3 class="cover-heading">Latest on Twitter</h3>

<div class="row">
  <% @tweets.each do |tweet| %>
    <div class="col-md-6">
      <div class="tech-news">
        <div class="card">
          <%= twitter_parser tweet %>
        </div>
      </div>
    </div>
  <% end %>
</div>
</div>