
<% if flash[:alert] %>
<%= js add_gritter(flash[:alert], title: "Joseph Brown Portfolio", sticky: false) %>
<% elsif flash[:error] %>
<%= js add_gritter(flash[:error], title: "Joseph Brown Portfolio", sticky: false) %>
<% else %>
<%= js add_gritter(flash[:notice], title: "Joseph Brown Portfolio", sticky: false) %>
<% end %>